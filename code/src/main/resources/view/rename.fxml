<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchor" prefHeight="720.0" prefWidth="1280.0"
            style="-fx-background-color: #e8e8e8;" stylesheets="@../style/style.css"
            xmlns="http://javafx.com/javafx/11.0.1" fx:controller="uk.co.conoregan.controller.RenameController">
    <children>
        <HBox prefHeight="720.0" prefWidth="1280.0">
            <children>
                <VBox alignment="CENTER" prefHeight="720.0" prefWidth="125.0" spacing="10.0">
                    <children>
                        <Button fx:id="buttonMenuRename" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenuSelected" text="Rename" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/rename_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="buttonMenuSearch" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenu" text="Search" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/search_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="buttonMenuSettings" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenu" text="Settings" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/settings_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox prefHeight="720.0" prefWidth="1155.0" spacing="20.0">
                    <children>
                        <HBox spacing="10.0">
                            <children>
                                <VBox spacing="10.0">
                                    <children>
                                        <HBox alignment="CENTER" prefWidth="200.0">
                                            <children>
                                                <Label text="Rename From">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                    <HBox.margin>
                                                        <Insets/>
                                                    </HBox.margin>
                                                </Label>
                                            </children>
                                        </HBox>
                                        <ListView fx:id="listViewRenameFrom" prefHeight="610.0" prefWidth="565.0">
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </ListView>
                                        <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Button fx:id="buttonOpenFileDialog" mnemonicParsing="false"
                                                        onAction="#openFileDialog" prefHeight="30.0" text="Add Source"/>
                                                <CheckBox mnemonicParsing="false" text="Include Sub-Folders"/>
                                                <Button fx:id="buttonGetRenameSuggestions" mnemonicParsing="false"
                                                        onAction="#getSuggestedNames" prefHeight="30.0"
                                                        text="Get Rename Suggestions"/>
                                            </children>
                                        </HBox>
                                    </children>
                           </VBox>
                                <VBox spacing="10.0">
                                    <children>
                                        <HBox alignment="CENTER" prefWidth="200.0">
                                            <children>
                                                <Label text="Rename To">
                                                    <font>
                                                        <Font size="20.0"/>
                                                    </font>
                                                </Label>
                                            </children>
                                        </HBox>
                                        <ListView fx:id="listViewRenameTo" prefHeight="610.0" prefWidth="565.0">
                                            <padding>
                                                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
                                            </padding>
                                        </ListView>
                                        <HBox alignment="CENTER_LEFT" prefWidth="200.0" spacing="20.0">
                                            <children>
                                                <Button fx:id="buttonRenameSelected" mnemonicParsing="false"
                                                        text="Rename Selected"/>
                                                <Button fx:id="buttonRenameAll" mnemonicParsing="false"
                                                        onAction="#renameAll" text="Rename ALL"/>
                                            </children>
                                            <VBox.margin>
                                                <Insets/>
                                            </VBox.margin>
                                        </HBox>
                                    </children>
                        </VBox>
                     </children>
                  </HBox>
               </children>
               <HBox.margin>
                   <Insets bottom="10.0" right="10.0" top="10.0"/>
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
