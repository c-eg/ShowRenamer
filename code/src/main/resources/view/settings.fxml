<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="anchor" prefHeight="720.0" prefWidth="1280.0"
            style="-fx-background-color: #e8e8e8;" stylesheets="@../style/style.css"
            xmlns="http://javafx.com/javafx/11.0.1"
            fx:controller="uk.co.conoregan.showrenamer.controller.SettingsController">
    <children>
        <HBox prefHeight="720.0" prefWidth="1280.0">
            <children>
                <VBox alignment="CENTER" prefHeight="720.0" prefWidth="125.0" spacing="10.0">
                    <children>
                        <Button fx:id="buttonMenuRename" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenu" text="Rename" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/rename_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="buttonMenuSearch" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenu" text="Search" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/search_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                        <Button fx:id="buttonMenuSettings" alignment="CENTER" contentDisplay="TOP" graphicTextGap="5.0"
                                maxWidth="1.7976931348623157E308" mnemonicParsing="false" prefWidth="150.0"
                                styleClass="buttonMenuSelected" text="Settings" textAlignment="CENTER">
                            <font>
                                <Font size="16.0"/>
                            </font>
                            <graphic>
                                <ImageView fitHeight="50.0" fitWidth="50.0" pickOnBounds="true" preserveRatio="true">
                                    <image>
                                        <Image url="@../images/settings_white.png"/>
                                    </image>
                                </ImageView>
                            </graphic>
                            <padding>
                                <Insets bottom="5.0" top="15.0"/>
                            </padding>
                        </Button>
                    </children>
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <HBox.margin>
                        <Insets left="10.0" right="10.0"/>
                    </HBox.margin>
                </VBox>
                <VBox maxHeight="720.0" prefHeight="720.0" prefWidth="1155.0" spacing="20.0">
                    <children>
                        <HBox spacing="10.0" VBox.vgrow="ALWAYS">
                            <children>
                                <Label text="Settings"/>
                            </children>
                        </HBox>
                    </children>
                    <HBox.margin>
                        <Insets bottom="10.0" right="10.0" top="10.0"/>
                    </HBox.margin>
                </VBox>
            </children>
        </HBox>
    </children>
</AnchorPane>
